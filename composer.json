{
  "name": "erkineren/opencart-model-invoker",
  "description": "Access all opencart model's methods and invoke them remotely, use it like a api",
  "config": {
    "platform": {
      "php": "5.6"
    },
    "process-timeout": 0,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "OpencartApi\\": "src/"
    },
    "classmap": [
      "config/autoload/"
    ]
  },
  "require": {
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3",
    "vlucas/phpdotenv": "^3.6"
  }
}
